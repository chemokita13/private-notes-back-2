{
    "version": 2,
    "env": {
        "MONGODBURI": "@MONGODBURI",
        "PGUSER":"@PGUSER",
        "PGHOST":"@PGHOST",
        "PGPORT":"@PGPORT",
        "PGPASSWORD":"@PGPASSWORD",
        "PGDATABASE":"@PGDATABASE"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}